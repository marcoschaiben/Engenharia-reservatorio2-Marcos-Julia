noprefix "false"

\end_inset

 e 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:taxaliquidagerada"
plural "false"
caps "false"
noprefix "false"

\end_inset

 na Equação 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:1"
plural "false"
caps "false"
noprefix "false"

\end_inset

, obtêm-se:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\underset{\text{\{Taxa de Acúmulo de i em V\}}}{\underset{V}{\int}\frac{\partial W_{i}}{\partial t}dV}=\underset{\{\text{Taxa de entrada e saída de i em V}\}}{-\underset{A}{\int}\overrightarrow{n}.\overrightarrow{N_{i}}\Delta A}+\underset{\{\text{Taxa de i gerada em V}\}}{\underset{V}{\int}R_{i}dV\text{ },i=1,2,...,N_{c}}\label{eq:7}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Aplicando o Teorema da Divergência na Equação 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:7"
plural "false"
caps "false"
noprefix "false"

\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\underset{A}{\int}\overrightarrow{n}.\overrightarrow{N_{i}}\Delta A=\underset{V}{\int}\overrightarrow{\nabla}N_{i}dV
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\underset{V}{\int}\left(\frac{\partial W_{i}}{\partial t}+\overrightarrow{\nabla}\overrightarrow{N_{i}}-R_{i}\right)dV=0\text{ },i=1,2,...,N_{c}\label{eq:7-1}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Considerando o Volume (V) arbitrário, a equação da conservação do componente
 i na sua forma diferencial reduz-se a:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{\partial W_{i}}{\partial t}+\overrightarrow{\nabla}\overrightarrow{N_{i}}-R_{i}=0\text{ },i=1,2,...,N_{c}\label{eq:equacao_conservacao_i_em_v}
\end{equation}

\end_inset


\end_layout

\begin_layout Subsubsection*

\series bold
Definição de termos para o fluxo isotérmico em meios porosos
\end_layout

\begin_layout Standard
Com o objetivo de desenvolver o estudo do fluxo multifásico em meios porosos,
 nesta subseção, serão definidos uma série de termos para posterior uso
 na solução de problemas deste caráter.
\end_layout

\begin_layout Standard
Definindo a porosidade como:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\phi=\frac{V_{p}}{V_{b}}\label{eq:porosidade}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
onde 
\begin_inset Formula $\phi=$
\end_inset

porosidade, 
\begin_inset Formula $V_{p}=$
\end_inset

volume poroso e 
\begin_inset Formula $V_{b}=$
\end_inset

volume total.
\end_layout

\begin_layout Standard
Se 
\begin_inset Formula $S_{j}=$
\end_inset

fração do volume de poros ocupado pela fase j e 
\begin_inset Formula $E_{j}=$
\end_inset

fração de volume da fase:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
S_{j}=\frac{V_{j}}{V_{p}}\label{eq:saturacao_fase_j}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
E_{j}=\frac{V_{j}}{V_{b}}\label{eq:fracao_de_volume_de_j}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Substituindo as Equações 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:saturacao_fase_j"
plural "false"
caps "false"
noprefix "false"

\end_inset

 e 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:porosidade"
plural "false"
caps "false"
noprefix "false"

\end_inset

 na Equação 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:fracao_de_volume_de_j"
plural "false"
caps "false"
noprefix "false"

\end_inset

, obtêm-se o conteúdo de fluido:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
E_{j}=\frac{S_{j}V_{p}}{\frac{V_{p}}{\phi}}=S_{j}\phi
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Para a fase sólida:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
E_{s}=1-\phi
\end{equation}

\end_inset


\end_layout

\begin_layout Standard

\family roman
\series medium
\shape up
\size normal
