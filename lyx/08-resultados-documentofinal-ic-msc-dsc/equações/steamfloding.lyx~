\begin_layout Standard
O termo 
\begin_inset Formula $(\hat{H_{3}}-\hat{H_{1}})$
\end_inset

 é o 
\begin_inset Formula $L_{v},$
\end_inset

 o calor latente de vaporização de água, e assumimos que as entalpias são
 independentes da pressão, 
\begin_inset Formula $dH_{j}=C_{pj}dT$
\end_inset

, onde 
\begin_inset Formula $C_{pj}$
\end_inset

 é o calor específico da fase j.
 Se o 
\begin_inset Formula $C_{pj}$
\end_inset

 for constante, Eq.
 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:derivada"
plural "false"
caps "false"
noprefix "false"

\end_inset

 torna-se:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline*}
\frac{\partial T}{\partial t}\left[\phi(\rho_{1}C_{p1}S_{1}+\rho_{2}S_{2}C_{p2})+(1-\phi)\rho_{s}C_{ps}\right]+(\rho_{1}C_{p1}\overrightarrow{u_{1}}+\rho_{2}C_{p2}\overrightarrow{u_{2}})\cdotp\overrightarrow{\nabla}T-\overset{\rightarrow}{\nabla}\cdotp\left(k_{T_{t}}\overset{\rightarrow}{\nabla}T\right)
\end{multline*}

\end_inset


\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\xout off
\uuline off
\uwave off
\noun off
\color none

\begin_inset Formula 
\begin{multline}
-L_{v}\left[\phi\frac{\partial(\text{\ensuremath{\rho_{3}S_{3})}}}{\partial t}+\overset{\rightarrow}{\nabla}\cdotp(\rho_{3}S_{3}\overrightarrow{u_{3}})\right]\label{eq:final}
\end{multline}

\end_inset


\end_layout

\begin_layout Standard
definindo 
\begin_inset Formula $M_{Tt}$
\end_inset

como a capacidade de calor volumétrica geral,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
M_{Tt}=\phi(\rho_{1}C_{p1}S_{1}+\rho_{2}S_{2}C_{p2})+(1-\phi)\rho_{s}C_{ps}
\]

\end_inset


\end_layout

\begin_layout Standard
então a Eq.
 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:final"
plural "false"
caps "false"
noprefix "false"

\end_inset

, se torna:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline*}
\frac{\partial T}{\partial t}M_{Tt}+(\rho_{1}C_{p1}\overrightarrow{u_{1}}+\rho_{2}C_{p2}\overrightarrow{u_{2}})\cdotp\overrightarrow{\nabla}T-\overset{\rightarrow}{\nabla}\cdotp\left(k_{T_{t}}\overset{\rightarrow}{\nabla}T\right)=-L_{v}\left[\phi\frac{\partial(\text{\ensuremath{\rho_{3}S_{3})}}}{\partial t}+\overset{\rightarrow}{\nabla}\cdotp(\rho_{3}S_{3}\overrightarrow{u_{3}})\right]
\end{multline*}

\end_inset


\end_layout

