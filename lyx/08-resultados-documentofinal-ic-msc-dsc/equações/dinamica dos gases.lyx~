\begin_body

\begin_layout Standard

\series bold
\lang english
Injeção de Água quente
\end_layout

\begin_layout Standard

\lang english
Relembrando as equações:
\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\[
\frac{\partial S_{1}}{\partial t}+\frac{u_{t}}{\phi}\frac{\partial f_{1}(S_{1},T)}{\partial x}=0
\]

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\[
\frac{\partial}{\partial t}\left[\phi\left(\rho_{1}S_{1}H_{1}+\rho_{2}S_{2}H_{2}\right)+(1-\phi)\rho_{s}H_{s}\right]+\frac{\partial}{\partial x}\cdotp\left(\rho_{1}\overrightarrow{u_{1}}H_{1}+\rho_{2}\overrightarrow{u_{2}}H_{2}\right)=0
\]

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout

\lang english
\begin_inset Formula 
\begin{equation}
\frac{\partial S_{1}}{\partial t}+\frac{u_{t}}{\phi}\frac{\partial f_{1}(S_{1},T)}{\partial x}=0\label{eq:1}
\end{equation}

\end_inset


\end_layout

\begin_layout Plain Layout

\lang english
\begin_inset Formula 
\begin{equation}
\left[\phi\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+(1-\phi)M_{t_{s}}\right]\frac{\partial T}{\partial t}+\left[u_{t}(M_{t_{1}}f_{1}+M_{t_{2}}f_{2})\right]\frac{\partial T}{\partial x}=0\label{eq:2}
\end{equation}

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard

\lang english
Pela Regra da Cadeia, a Equação 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:1"
plural "false"
caps "false"
noprefix "false"

\end_inset

 pode ser reescrita como:
\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\begin{equation}
\frac{\partial S_{1}}{\partial t}+\frac{u_{t}}{\phi}\left[\frac{\partial f_{1}}{\partial S_{1}}\frac{\partial S_{1}}{\partial x}+\frac{\partial f_{1}}{\partial T}\frac{\partial T}{\partial x}\right]=0\label{eq:1-1}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard

\lang english
As Equações 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:2"
plural "false"
caps "false"
noprefix "false"

\end_inset

 e 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:1-1"
plural "false"
caps "false"
noprefix "false"

\end_inset

 podem ser representadas na forma matricial:
\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\begin{equation}
\left[\begin{array}{cc}
1 & 0\\
0 & 1
\end{array}\right]\left[\begin{array}{c}
\frac{\partial S_{1}}{\partial t}\\
\frac{\partial T}{\partial t}
\end{array}\right]+\left[\begin{array}{cc}
\frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial S_{1}} & \frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial T}\\
0 & \frac{u_{t}}{\phi}\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]}
\end{array}\right]\left[\begin{array}{c}
\frac{\partial S_{1}}{\partial x}\\
\frac{\partial T}{\partial x}
\end{array}\right]=\left[\begin{array}{c}
0\\
0
\end{array}\right]
\end{equation}

\end_inset


\end_layout

\begin_layout Standard

\lang english
sendo 
\begin_inset Formula $M_{T_{j}}=\text{\ensuremath{\rho_{j}}}C_{P_{j}}$
\end_inset


\end_layout

\begin_layout Itemize

\series bold
\lang english
Autovalores ( velocidades características ):
\end_layout

\begin_layout Standard

\lang english
A partir da correlação de autovalores e autovetores: 
\begin_inset Formula $(A-\lambda I)\overrightarrow{r}=0:$
\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\begin{equation}
\left[\begin{array}{cc}
\frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial S_{1}} & \frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial T}\\
0 & \frac{u_{t}}{\phi}\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]}
\end{array}\right]-\left[\begin{array}{cc}
\lambda & 0\\
0 & \lambda
\end{array}\right]=\left[\begin{array}{c}
0\\
0
\end{array}\right]
\end{equation}

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\begin{equation}
\left[\begin{array}{cc}
\frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial S_{1}}-\lambda & \frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial T}\\
0 & \frac{u_{t}}{\phi}\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]}-\lambda
\end{array}\right]=\left[\begin{array}{c}
0\\
0
\end{array}\right]
\end{equation}

\end_inset


\end_layout

\begin_layout Standard

\lang english
Como:
\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\begin{equation}
det(A-\lambda I)=0
\end{equation}

\end_inset


\end_layout

\begin_layout Standard

\lang english
então:
\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\begin{equation}
\left(\frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial S_{1}}-\lambda\right)\left(\frac{u_{t}}{\phi}\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]}-\lambda\right)=0
\end{equation}

\end_inset


\end_layout

\begin_layout Standard

\lang english
Assim, os dois autovalores(
\begin_inset Formula $\lambda_{1}\text{e}\text{\ensuremath{\lambda_{2}}}$
\end_inset

) serão:
\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\begin{equation}
\lambda_{1}=\frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial S_{1}}\label{eq:autovalor1}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\begin{equation}
\lambda_{2}=\frac{u_{t}}{\phi}\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]}\label{eq:autovalor2}
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize

\series bold
\lang english
Autovetores
\end_layout

\begin_layout Standard

\lang english
