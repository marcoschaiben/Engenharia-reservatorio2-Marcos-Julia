\end_inset

:
\end_layout

\begin_layout Itemize

\series bold
\lang english
Para 
\begin_inset Formula $\lambda_{1}=\frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial S_{1}}$
\end_inset

:
\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\begin{equation}
\left[\begin{array}{cc}
\frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial S_{1}}-\frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial S_{1}} & \frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial T}\\
0 & \frac{u_{t}}{\phi}\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]}-\frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial S_{1}}
\end{array}\right]\left[\begin{array}{c}
r_{1}^{(1)}\\
r_{1}^{(2)}
\end{array}\right]=\left[\begin{array}{c}
0\\
0
\end{array}\right]
\end{equation}

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\begin{equation}
\begin{cases}
0r_{1}^{(1)}+ & \frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial T}r_{1}^{(2)}=0\\
0r_{1}^{(1)}+ & \frac{u_{t}}{\phi}\left(\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]}-\frac{\partial f_{1}}{\partial S_{1}}\right)r_{1}^{(2)}=0
\end{cases}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\begin{equation}
\overrightarrow{r_{1}}=\left[\begin{array}{c}
r_{1}^{(1)}\\
r_{1}^{(2)}
\end{array}\right]=\left[\begin{array}{c}
1\\
0
\end{array}\right]
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize

\series bold
\lang english
Para 
\begin_inset Formula $\lambda_{2}=\frac{u_{t}}{\phi}\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]}$
\end_inset

:
\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula $\left[\begin{array}{cc}
\frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial S_{1}}-\frac{u_{t}}{\phi}\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]} & \frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial T}\\
0 & \frac{u_{t}}{\phi}\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]}-\frac{u_{t}}{\phi}\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]}
\end{array}\right]\left[\begin{array}{c}
r_{2}^{(1)}\\
r_{2}^{(2)}
\end{array}\right]=\left[\begin{array}{c}
0\\
0
\end{array}\right]$
\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\begin{equation}
\begin{cases}
\frac{u_{t}}{\phi}\left(\frac{\partial f_{1}}{\partial S_{1}}-\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]}\right)r_{2}^{(1)}+ & \frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial T}r_{2}^{(2)}=0\\
0r_{2}^{(1)} & +0r_{2}^{(2)}=0
\end{cases}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\begin{equation}
\begin{cases}
\left(\frac{\partial f_{1}}{\partial S_{1}}-\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]}\right)r_{2}^{(1)}+ & \frac{\partial f_{1}}{\partial T}r_{2}^{(2)}=0\\
0r_{2}^{(1)} & +0r_{2}^{(2)}=0
\end{cases}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard

