\end{array}\right]=\left[\begin{array}{c}
\frac{\partial f_{1}}{\partial T}\\
\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]}-\frac{\partial f_{1}}{\partial S_{1}}
\end{array}\right]
\end{equation}

\end_inset


\end_layout

\begin_layout Subsection*

\series bold
\lang english
Condições de Choque:
\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\[
\frac{\partial S_{1}}{\partial t}+\frac{u_{t}}{\phi}\frac{\partial f_{1}(S_{1},T)}{\partial x}=0\rightarrow\frac{\partial S_{1}}{\partial t}+\frac{\partial\frac{u_{t}}{\phi}f_{1}(S_{1},T)}{\partial x}=0
\]

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\[
D=\frac{[\frac{u_{t}}{\phi}f_{1}(S_{1},T)]}{[S_{1}]}\rightarrow D=\frac{u_{t}}{\phi}\frac{[f_{1}]}{[S_{1}]}\rightarrow D=\frac{u_{t}}{\phi}\frac{(f_{1}^{+}-f_{1}^{-})}{(S_{1}^{+}-S_{1}^{-})}
\]

\end_inset


\end_layout

\begin_layout Itemize

\lang english
\begin_inset Formula $\frac{\partial}{\partial t}\left[\phi\left(\rho_{1}S_{1}H_{1}+\rho_{2}S_{2}H_{2}\right)+(1-\phi)\rho_{s}H_{s}\right]+\frac{\partial}{\partial x}\cdotp\left(\rho_{1}\overrightarrow{u_{1}}H_{1}+\rho_{2}\overrightarrow{u_{2}}H_{2}\right)=0$
\end_inset


\end_layout

\begin_layout Itemize

\lang english
\begin_inset Formula $\frac{\partial}{\partial t}\left[\phi\left(\rho_{1}S_{1}C_{1}T+\rho_{2}S_{2}C_{2}T\right)+(1-\phi)\rho_{s}C_{S}T\right]+\frac{\partial}{\partial x}\cdotp\left(\rho_{1}\overrightarrow{u_{1}}C_{1}T+\rho_{2}\overrightarrow{u_{2}}C_{2}T\right)=0$
\end_inset


\end_layout

\begin_layout Itemize

\lang english
\begin_inset Formula $\frac{\partial}{\partial t}\left[\phi\left(M_{t_{1}}S_{1}T+M_{t_{2}}S_{2}T\right)+(1-\phi)M_{ts}T\right]+\frac{\partial}{\partial x}\cdotp\left(u_{t}\left(M_{t_{1}}f_{1}T+M_{t_{2}}f_{2}T\right)\right)=0$
\end_inset


\end_layout

\begin_layout Itemize

\lang english
\begin_inset Formula $\frac{\partial}{\partial t}\left[\phi\left(M_{t_{1}}S_{1}T+M_{t_{2}}(1-S_{1})T\right)+(1-\phi)M_{ts}T\right]+\frac{\partial}{\partial x}\cdotp\left(u_{t}\left(M_{t_{1}}f_{1}T+M_{t_{2}}(1-f_{1})T\right)\right)=0$
\end_inset


\end_layout

\begin_layout Itemize

\lang english
\begin_inset Formula $\frac{\partial}{\partial t}\left[\phi\left(M_{t_{1}}S_{1}T-M_{t_{2}}S_{1}T+M_{t_{2}}T\right)+(1-\phi)M_{ts}T\right]+\frac{\partial}{\partial x}\cdotp\left(u_{t}\left(M_{t_{1}}f_{1}T+M_{t_{2}}T-M_{t_{2}}Tf_{1}\right)\right)=0$
\end_inset


\end_layout

\begin_layout Itemize

\lang english
\begin_inset Formula $\frac{\partial}{\partial t}\left[\phi\left((M_{t_{1}}-M_{t_{2}})S_{1}T+M_{t_{2}}T\right)+(1-\phi)M_{ts}T\right]+\frac{\partial}{\partial x}\cdotp\left(u_{t}\left((M_{t_{1}}-M_{t_{2}})f_{1}T+M_{t_{2}}T\right)\right)=0$
\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula 
\[
D=\frac{u_{t}}{\phi}\frac{\left((M_{t_{1}}-M_{t_{2}})(f_{1}^{+}T^{+}-f_{1}^{-}T^{-})+M_{t_{2}}(T^{+}-T^{-})\right)}{\left(\left((M_{t_{1}}-M_{t_{2}})(S_{1}^{+}T^{+}-S_{1}^{-}T^{-})+M_{t_{2}}(T^{+}-T^{-})\right)+\frac{(1-\phi)}{\phi}M_{ts}(T^{+}-T^{-})\right)}
\]

\end_inset


\end_layout

\begin_layout Subsubsection*

\lang english
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout

\lang english
\begin_inset Formula 
\[
D=\frac{\left[\left(\rho_{1}\overrightarrow{u_{1}}^{+}H_{1}^{+}+\rho_{2}\overrightarrow{u_{2}}^{+}H_{2}^{+}\right)\right]}{\left[\phi\left(\rho_{1}H_{1}^{+}S_{1}^{+}+\rho_{2}H_{2}^{+}(1-S_{1}^{+})\right)+(1-\phi)\rho_{s}H_{s}^{+}\right]}-\frac{\left[\left(\rho_{1}\overrightarrow{u_{1}}^{-}H_{1}^{-}+\rho_{2}\overrightarrow{u_{2}}^{-}H_{2}^{-}\right)\right]}{\left[\phi\left(\rho_{1}H_{1}^{-}S_{1}^{-}+\rho_{2}H_{2}^{-}(1-S_{1}^{-})\right)+(1-\phi)\rho_{s}H_{s}^{-}\right]}
\]

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Subsubsection*

\lang english
Famílias de Rarefação: 
\end_layout

\begin_layout Subsubsection*

\lang english
primeira família de rarefação:
\end_layout

\begin_layout Itemize

\lang english
\begin_inset Formula $\alpha_{1}=\left(\frac{\partial\lambda_{1}}{\partial S_{1}}r_{1}^{(1)}+\frac{\partial\lambda_{1}}{\partial T}r_{1}^{(2)}\right)^{-1}$
\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula $\alpha_{1}=\left(\frac{\partial}{\partial S_{1}}\left(\frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial S_{1}}\right)\cdot1+\frac{\partial}{\partial T}\left(\frac{u_{t}}{\phi}\frac{\partial f_{1}}{\partial S_{1}}\right)\cdot0\right)^{-1}\rightarrow\left(\frac{u_{t}}{\phi}\frac{\partial^{2}f_{1}}{\partial S_{1}^{2}}\right)^{-1}=\frac{1}{\frac{u_{t}}{\phi}\frac{\partial^{2}f_{1}}{\partial S_{1}^{2}}}$
\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula $\frac{du}{d\xi}=\alpha\overrightarrow{r}$
\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula $\frac{dS}{d\xi}=\frac{1}{\frac{u_{t}}{\phi}\frac{\partial^{2}f_{1}}{\partial S_{1}^{2}}}$
\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula $\frac{dT}{d\xi}=0$
\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula $\frac{dT}{dS}=0$
\end_inset


\end_layout

\begin_layout Subsubsection*

\lang english
Segunda família de rarefação:
\end_layout

\begin_layout Itemize

\lang english
\begin_inset Formula $\alpha_{2}=\left(\frac{\partial\lambda_{2}}{\partial S_{2}}r_{2}^{(1)}+\frac{\partial\lambda_{2}}{\partial T}r_{2}^{(2)}\right)^{-1}$
\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula $\alpha_{2}=\left[\frac{\partial f_{1}}{\partial T}\left[\frac{\partial}{\partial S_{1}}\left(\frac{u_{t}}{\phi}\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]}\right)\right]+\left(\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]}-\frac{\partial f_{1}}{\partial S_{1}}\right)\left[\frac{\partial}{\partial T}\left(\frac{u_{t}}{\phi}\frac{\left(M_{t_{1}}f_{1}+M_{t_{2}}f_{2}\right)}{\left[\left(M_{t_{1}}S_{1}+M_{t_{2}}S_{2}\right)+\frac{(1-\phi)}{\phi}M_{t_{s}}\right]}\right)\right]\right]^{-1}$
\end_inset


