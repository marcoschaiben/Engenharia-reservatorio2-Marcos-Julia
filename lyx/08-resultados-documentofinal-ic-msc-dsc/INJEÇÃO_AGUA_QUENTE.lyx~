\begin_inset Formula 
\begin{multline*}
(1-\phi)\frac{\partial(\rho_{s}H_{s})}{\partial t}+\phi\rho_{1}S_{1}\frac{\partial\hat{H_{1}}}{\partial t}+\phi\rho_{2}S_{2}\frac{\partial\hat{H_{2}}}{\partial t}+\rho_{1}\overrightarrow{u_{1}}\cdotp\overset{\rightarrow}{\nabla}\hat{H_{1}}+\rho_{2}\overrightarrow{u_{2}}\cdotp\overset{\rightarrow}{\nabla}\hat{H_{2}}
\end{multline*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline*}
+\hat{H_{1}}\left[\cancelto{0}{\overset{\rightarrow}{\nabla}\cdotp(\rho_{1}\overrightarrow{u_{1}})+\phi\frac{\partial(\rho_{1}S_{1})}{\partial t}}\right]+\hat{H_{2}}\left[\cancelto{0}{\overset{\rightarrow}{\nabla}\cdotp(\rho_{2}\overrightarrow{u_{2}})+\phi\frac{\partial(\rho_{2}S_{2})}{\partial t}}\right]=0
\end{multline*}

\end_inset


\end_layout

\begin_layout Standard
então,
\end_layout

\begin_layout Standard
\align left

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\xout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula 
\begin{multline*}
(1-\phi)\frac{\partial(\rho_{s}\hat{H_{s}})}{\partial t}+\phi\rho_{1}S_{1}\frac{\partial\hat{H_{1}}}{\partial t}+\phi\rho_{2}S_{2}\frac{\partial\hat{H_{2}}}{\partial t}+\rho_{1}\overrightarrow{u_{1}}\cdotp\overset{\rightarrow}{\nabla}\hat{H_{1}}+\rho_{2}\overrightarrow{u_{2}}\cdotp\overset{\rightarrow}{\nabla}\hat{H_{2}}=0
\end{multline*}

\end_inset


\end_layout

\begin_layout Standard
que considerando fluxo unidimensional,
\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\xout off
\uuline off
\uwave off
\noun off
